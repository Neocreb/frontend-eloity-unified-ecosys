    "test:supabase": "node test-supabase-integration.js",
    "test:replicate": "node scripts/test-replicate-function.js",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "migrate": "npx drizzle-kit generate:pg",
    "migrate:apply": "node scripts/apply-migrations-supabase.js",
    "db:studio": "npx drizzle-kit studio",
