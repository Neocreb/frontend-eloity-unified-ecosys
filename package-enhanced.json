{
  "name": "eloity-platform",
  "version": "1.0.0",
  "description": "Eloity - where everything connects. AI-powered social platform with marketplace, crypto trading, and freelancing features",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development npx tsx server/enhanced-index.ts",
    "dev:original": "NODE_ENV=development npx tsx server/index.ts",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "vite build",
    "build:backend": "esbuild server/enhanced-index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist --target=node18",
    "start": "NODE_ENV=production node dist/enhanced-index.js",
    "start:original": "NODE_ENV=production node dist/index.js",
    "check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:seed": "npx tsx scripts/seed.ts",
    "logs:view": "tail -f logs/combined.log",
    "logs:errors": "tail -f logs/error.log",
    "cache:flush": "npx tsx scripts/flush-cache.ts",
    "jobs:status": "npx tsx scripts/job-status.ts",
    "backup:db": "npx tsx scripts/backup-database.ts",
    "setup": "npm install && npm run db:generate && npm run db:push",
    "setup:env": "cp .env.example .env",
    "docker:build": "docker build -t eloity .",
    "docker:run": "docker run -p 5000:5000 eloity",
    "deploy:vercel": "vercel --prod",
    "analyze": "npm run build && npx webpack-bundle-analyzer dist/static/js/*.js"
  },
